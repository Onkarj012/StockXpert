{
  "expo": {
    "name": "StockXpert",
    "slug": "StockXpert",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true,
      "newArchEnabled": true
    },
    "jsEngine": "hermes",
    "plugins": [],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.StockXpert"
    },
    "extra": {
      "eas": {
        "projectId": "31ff34f8-c7b0-4655-bc75-5427cd99ec67"
      }
    },
    "owner": "leoeccentric"
  }
}
